{
  "extensions": [
    {
      "keyword": "weather",
      "pattern": "/weather\\s*(.+)",
      "className": "com.linangran.webchat.imp.plugins.WeatherPlugin"
    },
    {
      "keyword": "multiply",
      "pattern": "/multiply\\s+(\\d+)\\*(\\d.+)",
      "className": "com.linangran.webchat.imp.plugins.MultiplyPlugin",
      "url": "MultiplyPlugin.groovy"  //This Plugin is Dynamically loaded!
    },
    {
      "keyword": "reload",
      "pattern": "/reload",
      "className": "com.linangran.webchat.imp.plugins.ReloadPlugin" //The reload plugin will reload all plugins!
    }
  ]
}